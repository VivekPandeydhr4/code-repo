#include "header.h"

int nearPerfectSquare(int num)
{
  double n = 1;

  printf("squre of %lf = %lf", n, pow(n, 2));
  //while ()
  return 0;
}

int main()
{
  /*************************************************************/
  int arr[] = {1, 11, 0, 2, 4, 1, 6, 2, 1, 10, 9, -8, 1, 1};
